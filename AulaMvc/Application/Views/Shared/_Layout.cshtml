<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Application</title>
    <link href="~/lib/boobstrap4/boobst.js" rel="stylesheet"/>
    <link href ="~/css/site.css" />
</head>
<body>
   
    <div class="container mt-5">
        <main role="main" class="pb-3">
            @Application.Models.Notification.Get(TempData)
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - Application - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src ="~/lib/boobstrap4/boobst.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
